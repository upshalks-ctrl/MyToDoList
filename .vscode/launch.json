{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {"PYTHONPATH":"${workspaceRoot}"},
            "envFile": "${workspaceFolder}/.env",
            "cwd": "${fileDirname}"
        }
    ]
}